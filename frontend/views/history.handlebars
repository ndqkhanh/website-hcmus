{{> header }}
<div class='container'>
  <div class='history-list'>
    <nav aria-label='breadcrumb' class='bg-light rounded p-3'>
      <ol class='breadcrumb mb-0'>
        <li class='breadcrumb-item pb-0'><a href='#'>Home</a></li>
        <li class='breadcrumb-item active' aria-current='page'>History</li>
      </ol>
    </nav>
    <div class='h1 text-center'>Vexera history</div>
    <table class='table table-striped mt-5'>
      <tr>
        <th scope='col'>#</th>
        <th scope='col'>id</th>
        <th scope='col'>House</th>
        <th scope='col'>Start point</th>
        <th scope='col'>End point</th>
        <th scope='col'>Start time</th>
        <th scope='col'>End time</th>
        <th scope='col'>Details</th>
      </tr>
      <tr>
        <th scope='row'>1</th>
        <td>12c</td>
        <td>Phương Trang</td>
        <td>Miền Nam bus stop</td>
        <td>Miền Tây bus stop</td>
        <td>12/1/2022 16:00</td>
        <td>13/11/2022 15:00</td>
        <td>View</td>
      </tr>
      <tr>
        <th scope='row'>2</th>
        <td>12c</td>
        <td>Phương Trang</td>
        <td>Miền Nam bus stop</td>
        <td>Miền Tây bus stop</td>
        <td>12/1/2022 16:00</td>
        <td>13/11/2022 15:00</td>
        <td>View</td>
      </tr>
      <tr>
        <th scope='row'>3</th>
        <td>12c</td>
        <td>Phương Trang</td>
        <td>Miền Nam bus stop</td>
        <td>Miền Tây bus stop</td>
        <td>12/1/2022 16:00</td>
        <td>13/11/2022 15:00</td>
        <td>View</td>
      </tr>
    </table>
    <nav class='mt-5' aria-label='Page navigation example'>
      <ul class='pagination justify-content-center'>
        <li class='page-item'><a class='page-link' href='#'>Previous</a></li>
        <li class='page-item'><a class='page-link' href='#'>1</a></li>
        <li class='page-item'><a class='page-link' href='#'>2</a></li>
        <li class='page-item'><a class='page-link' href='#'>3</a></li>
        <li class='page-item'><a class='page-link' href='#'>Next</a></li>
      </ul>
    </nav>
  </div>

  <!-- View detail đang ẩn -->

  <div class='detail d-none'>
    <table class='table table-hover table-striped'>
      <tbody>
        <tr style='min-height: 50px'>
          <th class='quarter-width align-middle'>First name</th>
          <td class='quarter-width align-middle'>Mark</td>
          <th class='quarter-width align-middle'>Last name</th>
          <td class='quarter-width align-middle'>Zuckerberg</td>
        </tr>
        <tr style='min-height: 50px'>
          <th class='quarter-width align-middle'>Ticker id</th>
          <td class='quarter-width align-middle'>
            12ab, 12bc, 12cd, 12de, 12ef
          </td>
          <th class='quarter-width align-middle'>Bus operator</th>
          <td class='quarter-width align-middle'>Phương Trang</td>
        </tr>
        <tr style='min-height: 50px'>
          <th class='quarter-width align-middle'>Start point</th>
          <td class='quarter-width align-middle'>Miền Nam bus stop</td>
          <th class='quarter-width align-middle'>End point</th>
          <td class='quarter-width align-middle'>Miền Hoa bus stop</td>
        </tr>
        <tr style='min-height: 50px'>
          <th class='quarter-width align-middle'>Start time</th>
          <td class='quarter-width align-middle'>
            February 11th, 2022 15:00
          </td>
          <th class='quarter-width align-middle'>End time</th>
          <td class='quarter-width align-middle'>
            February 11th, 2022 20:00
          </td>
        </tr>
        <tr style='min-height: 50px'>
          <th class='quarter-width align-middle'>Duration</th>
          <td class='quarter-width align-middle'>5 hours</td>
          <th class='quarter-width align-middle'>Policy</th>
          <td class='quarter-width align-middle'>
            <ul class='none-list-style-type ps-0'>
              <li>
                WHILE ON BOARD
                <ul class='disc-list-style-type'>
                  <li>Holding your ticket</li>
                  <li>Be silent</li>
                </ul>
              </li>
              <li>
                YOUR PACKAGE
                <ul class='disc-list-style-type'>
                  <li>Not over 10kg</li>
                </ul>
              </li>
            </ul>
          </td>
        </tr>
        <tr style='min-height: 50px'>
          <th class='quarter-width align-middle'>Number of seats</th>
          <td class='quarter-width align-middle'>5</td>
          <th class='quarter-width align-middle'>Type of bus</th>
          <td class='quarter-width align-middle'>Vilahouse</td>
        </tr>
        <tr style='min-height: 50px'>
          <th class='quarter-width align-middle'>Ticket cost</th>
          <td class='quarter-width align-middle'>50k vnđ</td>
          <th class='quarter-width align-middle'>Total cost</th>
          <td class='quarter-width align-middle'>250k vnđ</td>
        </tr>
        <tr style='min-height: 50px'>
          <th class='quarter-width align-middle'>Status</th>
          <td class='quarter-width align-middle'>Booked</td>
          <th class='quarter-width align-middle'>Seat positions</th>
          <td class='quarter-width align-middle'>
            E10, E11, E12, E13, E16
          </td>
        </tr>
      </tbody>
    </table>
    <div class='mt-5 d-flex justify-content-center align-items-center'>
      <button type='button' class='btn btn-primary py-3 px-4'>
        Back
      </button>
    </div>
  </div>
</div>
{{> footer }}
<style>
  .container { width: 80%; }
</style>